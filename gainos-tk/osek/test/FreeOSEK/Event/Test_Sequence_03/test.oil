//OSEK OSEK {

OS	ExampleOS {
	STATUS = EXTENDED;
	PRETASKHOOK = FALSE;
	POSTTASKHOOK = FALSE;
   STARTUPHOOK = FALSE;
   ERRORHOOK = FALSE;
   SHUTDOWNHOOK = FALSE;
	MEMMAP = FALSE;
	USERESSCHEDULER = FALSE;
};

TASK Task1 {
   PRIORITY = 1;
   SCHEDULE = NON;
   ACTIVATION = 1;
   AUTOSTART = TRUE {
		APPMODE = OSDEFAULTAPPMODE;
	};
	STACK = 256;
	TYPE = BASIC;
};

TASK Task2 {
	PRIORITY = 2;
	SCHEDULE = NON;
   AUTOSTART = TRUE {
		APPMODE = OSDEFAULTAPPMODE;
	};
	ACTIVATION = 1;
	STACK = 256;
	TYPE = EXTENDED;
	EVENT = Event1;
	EVENT = Event2;
	EVENT = Event3;
}

TASK Task3 {
	PRIORITY = 3;
	SCHEDULE = NON;
   AUTOSTART = FALSE;
	ACTIVATION = 1;
	STACK = 256;
	TYPE = BASIC;
}

EVENT Event1{MASK=AUTO;};

EVENT Event2{MASK=AUTO;};

EVENT Event3{MASK=AUTO;};

