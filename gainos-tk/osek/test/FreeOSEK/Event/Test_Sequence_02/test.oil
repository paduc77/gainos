OS	ExampleOS {
	STATUS = EXTENDED;
	PRETASKHOOK = FALSE;
	POSTTASKHOOK = FALSE;
   STARTUPHOOK = FALSE;
   ERRORHOOK = FALSE;
   SHUTDOWNHOOK = FALSE;
	MEMMAP = FALSE;
	USERESSCHEDULER = FALSE;
};

TASK Task1 {
   PRIORITY = 1;
   SCHEDULE = FULL; //or NON
   ACTIVATION = 1;
	AUTOSTART = FALSE;
	STACK = 256;
	TYPE = EXTENDED;
	EVENT = Event1;
};

TASK Task2 {
	PRIORITY = 2;
	SCHEDULE = FULL; //or NON
	ACTIVATION = 1;
   AUTOSTART = TRUE {
		APPMODE = OSDEFAULTAPPMODE;
	};
	STACK = 256;
	TYPE = EXTENDED;
	EVENT = Event2;
}

EVENT Event1{MASK = AUTO;};

EVENT Event2{MASK = AUTO;};

