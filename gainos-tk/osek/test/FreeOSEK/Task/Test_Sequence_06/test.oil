OS	ExampleOS {
	STATUS = EXTENDED;
	PRETASKHOOK = FALSE;
	POSTTASKHOOK = FALSE;
   STARTUPHOOK = FALSE;
   ERRORHOOK = FALSE;
   SHUTDOWNHOOK = FALSE;
	MEMMAP = FALSE;
	USERESSCHEDULER = FALSE;
};

TASK Task1 {
   PRIORITY = 1;
   SCHEDULE = NON; //or FULL
   ACTIVATION = 1;
   AUTOSTART = TRUE {
		APPMODE = OSDEFAULTAPPMODE;
	};
	STACK = 256;
	TYPE = EXTENDED;
};

TASK Task2 {
	PRIORITY = 2;
	SCHEDULE = NON; //or FULL
	ACTIVATION = 1;
	AUTOSTART = FALSE;
	STACK = 256;
	TYPE = EXTENDED;
	EVENT = Event2;
}

EVENT Event2
{
	MASK = AUTO;
};

