OUTPUT_FORMAT("elf32-littlearm", "elf32-bigarm", "elf32-littlearm")
OUTPUT_ARCH(arm)

SECTIONS {
	.text 0x00000000 : AT(0x00000000) {
		*(.vector)
		*(.ftext)
		*(.ftext.1)
		*(.romtext.1)
		*(.ftext.2)
		*(.romtext.2)
		*(.ftext.3)
        *(.rodata.str1.4)
		*(.text)
		*(.rodata)
	} = 0
	 __exidx_start = .;
    .ARM.exidx :
    {
        *(.ARM.exidx* .gnu.linkonce.armexidx.*)
    } = 0
    __exidx_end = .;
	__data_org = .;
	.data 0x20001000 : AT(__data_org) {
		__data_start = .;
		*(.data)
		__data_end = .;
	}
	.bss : {
		__bss_start = .;
		PROVIDE (__noinit_start = .);
		*(.noinit)
		PROVIDE (__noinit_end = .);
		*(.bss)
		*(COMMON)
		__bss_end = .;
		__end = .;
	}
    /DISCARD/ :
    {
        *(.eh_*)
    }
}
